{
  "source": ["../properties/color/*.json"],
  "platforms": {
    "scss": {
      "transforms": [
			"attribute/cti",
			"name/cti/camel",
			"color/rgba"
		],
      "buildPath": "../dist/",
      "files": [
			{
			  "destination": "scss/_colors.scss",
			  "format": "scss/variables"
			}
		]
    },
    "android": {
      "transformGroup": "android",
      "buildPath": "../dist/",
      "files": [
			{
			  "destination": "android/colors.xml",
			  "template": "android/colors"
			}
		]
    }
  }
}
