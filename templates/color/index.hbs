<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8" />
	<title>swarm-constants</title>
	<link rel="stylesheet" type="text/css" href="https://meetup.github.io/sassquatch2/bundle/sassquatch.css" />
	<style type="text/css">
		body {
			font-family: Monaco, Consolas, monospace;
		}
		.darkBg {
			background-color:#0f1721;
		}
		.darkBg .catName,
		.darkBg label {
			color: white;
		}
		.colorSection {
			display: flex;
			flex-direction: column;
			justify-content: center;
		}
		.colorValue {
			cursor: pointer;
			padding: 6px 12px;
			min-width: 25% !important;
		}
		.colorValue:hover {
			background: rgba(255,255,255,0.5);
		}
		.colorValue:active {
			background: rgba(255,255,255,0.33);
		}

		.checkerBg {
			background-image: url(data:image/png;base64,);
		}
	</style>
</head>
<body>
	<div class="column" style="min-height: 100vh;">
		<section class="bounds align--right">
			<input style="height: auto; display: inline-block;" id="toggleBg" type="checkbox" />
			<label style="display: inline-block;" for="toggleBg">Dark background</label>
		</section>

		{{#each categories}}
			<section class="bounds margin--top padding--top">
				<h1 class="catName text--display2 chunk">{{name}}</h1>
			</section>
			{{#each colors}}
				<section class="bounds padding--none margin--bottom">
					<div class="checkerBg">
						<div class="padding--all {{getInvertedClass value}}" style="background-color: {{toRgba value}};">
							<div class="spreadable atMedium_spreadable--spread">
								<div class="spreadable-item valign--middle">
									<h2 class="text--big text--heavy">{{name}}</h2>
								</div>
								<dl
									title="Click to copy"
									class="spreadable-item spreadable-item--shrink colorValue valign--middle align--right atMedium_align--left"
									data-clipboard-target="#{{name}}_hex"
								>
									<dt class="text--secondary">HEX:</dt>
									<dd id="{{name}}_hex">{{toHex value}}</dd>
								</dl>
								<dl
									title="Click to copy"
									class="spreadable-item spreadable-item--shrink colorValue valign--middle align--right atMedium_align--left"
									data-clipboard-target="#{{name}}_rgba"
								>
									<dt class="text--secondary">RGBA:</dt>
									<dd id="{{name}}_rgba">{{toRgba value}}</dd>
								</dl>
							</div>
						</div>
					</div>
				</section>
			{{/each}}
		{{/each}}

	</div>

	<script
		src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
		integrity="sha256-k2WSCIexGzOj3Euiig+TlR8gA0EmPjuc79OEeY5L45g="
		crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/1.7.1/clipboard.min.js"></script>
	<script type="text/javascript">

		function onBgToggle($input, $body) {
			var useDarkBg = $input.prop('checked');
			if (useDarkBg) {
				$body.addClass('darkBg')
			} else {
				$body.removeClass('darkBg')
			}
		};

		$(function() {
			var body = $(document.body);
			new Clipboard('.colorValue');

			$('#toggleBg').on('change', function(e) {
				onBgToggle($(e.target), body);
			});
		});
	</script>
</body>
</html>
